    "lint-staged": "^10.1.3",
    "localforage": "^1.7.3",
    "ulog": "^2.0.0-beta.7",
    "vue": "^2.6.10",
    "vue-clickaway": "^2.1.0",
    "vue-notifyjs": "^0.3.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.3",
    "vuex-map-fields": "^1.4.0",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "node-sass": "^7.0.0",
